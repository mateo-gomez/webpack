{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "curso de webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack index.js ---mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-imagenes/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js --watch",
    "build:react": "webpack --config ./react/webpack.config.js --watch",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js --progress",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js --progress",
    "build:less": "webpack --config ./less-loader/webpack.config.js --progress",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js --progress",
    "build:prevent": "webpack --config ./prevent-duplication/webpack.config.js --progress",
    "build:vendor": "webpack --config ./vendor/webpack.config.js --progress",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js --progress",
    "build:dll:src": "webpack --config ./dll/webpack.config.js --progress",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js --progress"
  },
  "keywords": [
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nidreim/webpack.git"
  },
